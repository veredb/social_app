<h1>Home Page</h1>
<p>This is the home page</p>

<%= form_for(@user) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <%= f.label :address %>
    <%= f.text_field :address %>
    <%= f.label :phone %>
    <%= f.text_field :phone %>
    <%= f.submit 'submit' %>
<% end %>

